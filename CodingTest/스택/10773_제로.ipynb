    "import sys\n",
    "sys.stdin = open('test.txt', 'r')\n",
    "input = sys.stdin.readline\n",
    "\n",
    "K = int(input())\n",
    "lst = []\n",
    "for _ in range(K):\n",
    "    inp = int(input())\n",
    "    \n",
    "    if inp == 0:\n",
    "        lst.pop()\n",
    "        continue\n",
    "    \n",
    "    lst.append(inp)\n",
    "print(sum(lst))"
   ]
